<div>
  <h1>HOME</h1>
  <div class="container-products">
    {{#if products.length}}
      <ul>
        {{#each products}}
          <li>
            <h2>{{this.title}}</h2>
            <p>Description: {{this.description}}</p>
            <p>Price: ${{this.price}}</p>
            <p>Stock: {{this.stock}}</p>
            <p>Id: {{this.id}}</p>
          </li>
        {{/each}}
      </ul>
    {{else}}
      <p>No products available</p>
    {{/if}}
  </div>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="home.js"></script>